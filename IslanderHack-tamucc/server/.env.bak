# Node.js Server Environment Variables
 
# Server Configuration
PORT=5000
NODE_ENV=development
 
# Google Maps API Key (required for maps services)
GOOGLE_MAPS_API_KEY=***REMOVED***
 
# Python FastAPI Backend URL (for AI services)
PYTHON_API_URL=http://localhost:8000
 
# OpenAI API Key (for AI-powered notifications)
OPENAI_API_KEY=***REMOVED***
 
# OpenWeather API Key (for weather data and hazard analysis)
# Sign up at https://openweathermap.org/api
# Free tier includes current weather, forecasts, and alerts
OPENWEATHER_API_KEY=your_openweather_api_key_here
 
# ==================== NOTIFICATION SETTINGS ====================
 
# Enable/Disable Alert Channels (true/false)
ENABLE_EMAIL_ALERTS=true
ENABLE_PUSH_ALERTS=true
ENABLE_SMS_ALERTS=false
 
